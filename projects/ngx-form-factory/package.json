{
    "name": "@ezzabuzaid/ngx-form-factory",
    "version": "0.7.0",
    "peerDependencies": {
        "@angular/common": "^13.1.0",
        "@angular/core": "^13.1.0",
        "@angular/material": "^13.0.3",
        "@angular/cdk": "^13.0.3",
        "@angular/forms": "~13.1.0",
        "@ngneat/input-mask": "^5.1.0",
        "inputmask": "^5.0.7"
    },
    "dependencies": {
        "tslib": "^2.3.0"
    },
    "bugs": {
        "email": "ezzabuzaid@hotmail.com",
        "url": "https://github.com/ezzabuzaid/ngx-form-factory/issues"
    },
    "description": "Dynamic angular reactive form creation with material design",
    "keywords": [
        "Angular",
        "Forms",
        "Reactive forms",
        "Create form on runtime",
        "json based forms",
        "ezzabuzaid"
    ],
    "author": {
        "name": "ezzabuzaid",
        "email": "ezzabuzaid@hotmail.com",
        "url": "https://github.com/ezzabuzaid/"
    },
    "repository": "github:ezzabuzaid/ngx-form-factory",
    "private": false,
    "license": "MIT",
    "release-it": {
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "conventionalcommits",
                "infile": "./../../CHANGELOG.md"
            }
        },
        "git": {
            "requireBranch": "master",
            "requireCleanWorkingDir": true,
            "requireCommits": true,
            "requireUpstream": true,
            "tag": true
        },
        "github": {
            "release": false
        },
        "npm": {
            "publish": false,
            "publishPath": "./../../dist"
        },
        "hooks": {
            "before:init": [],
            "after:bump": "npm run build",
            "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
        }
    },
    "scripts": {
        "release": "release-it",
        "build": "ng build",
        "prebuild": "del-cli ./dist/"
    }
}
