{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-form-factory": {
      "projectType": "library",
      "root": "projects/ngx-form-factory",
      "sourceRoot": "projects/ngx-form-factory/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ngx-form-factory/tsconfig.lib.json",
            "project": "projects/ngx-form-factory/ng-package.json"
          },
          "defaultConfiguration": "production",
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-form-factory/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-form-factory/src/test.ts",
            "tsConfig": "projects/ngx-form-factory/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-form-factory/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-form-factory",
  "cli": {
    "analytics": "d4e4ba91-0edc-4c39-acc2-abc79ea46255"
  }
}