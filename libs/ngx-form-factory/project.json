{
  "projectType": "library",
  "root": "libs/ngx-form-factory",
  "sourceRoot": "libs/ngx-form-factory/src",
  "prefix": "ngx",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/ngx-form-factory"],
      "options": {
        "project": "libs/ngx-form-factory/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ngx-form-factory/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ngx-form-factory/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ngx-form-factory"],
      "options": {
        "jestConfig": "libs/ngx-form-factory/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ngx-form-factory/src/**/*.ts",
          "libs/ngx-form-factory/src/**/*.html"
        ]
      }
    },
    "release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "v",
        "baseBranch": "master",
        "push": true,
        "postTargets": ["ngx-form-factory:github", "ngx-form-factory:publish"]
      }
    },
    "pre-release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "tagPrefix": "",
        "baseBranch": "beta",
        "releaseAs": "prerelease",
        "preid": "beta",
        "push": true,
        "postTargets": ["ngx-form-factory:github", "ngx-form-factory:publish"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "buildTarget": "production"
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "staticDir": [
          "node_modules/flag-icons",
          "node_modules/flag-icons/css",
          "node_modules/intl-tel-input/build/js",
          "node_modules/@angular/material/prebuilt-themes"
        ],
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/ngx-form-factory/.storybook"
        },
        "projectBuildConfig": "ngx-form-factory:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/ngx-form-factory",
        "config": {
          "configFolder": "libs/ngx-form-factory/.storybook"
        },
        "projectBuildConfig": "ngx-form-factory:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
